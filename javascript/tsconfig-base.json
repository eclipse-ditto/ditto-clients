{
  "compilerOptions": {
    "declaration": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "declarationMap": true,
    "maxNodeModuleJsDepth": 0,
    "skipLibCheck": true
  }
}
